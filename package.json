{
  "name": "mandarinenfilm",
  "version": "0.1.0",
  "scripts": {
    "start": "chokidar \"sass/**/*.scss\" -c \"npm run build\" --initial",
    "build": "node-sass sass/style.scss style.css --output-style compact --indent-type tab --indent-width 1 --source-map true",
    "prod": "node-sass sass/style.scss style.css --output-style compressed --source-map true"
  },
  "author": "Florian Geierstanger",
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^6.0.0",
    "chokidar-cli": "^2.1.0"
  }
}